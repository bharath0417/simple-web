{
    "family": "aws-task-latest",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "aws-task-latest",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "networkMode": "awsvpc",
            "requiresCompatibilities": [ 
                "FARGATE" ]
            "portMappings": [
                {
		    "containerPort": 8088,
                    "hostPort": 8088
                }
            ]
        }
    ]
}